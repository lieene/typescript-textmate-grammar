{
  "name": "tm-grammar",
  "version": "1.0.1",
  "description": "textmate grammar",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"out/**/*.ts\" \"out/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "update": "npm i text-editing oniguruma-ext && npm run test",
    "fastpatch": "git add . && git commit && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lieene/typescript-textmate-grammar.git"
  },
  "keywords": [
    "textmate",
    "grammar",
    "oniguruma",
    "syntax",
    "tree"
  ],
  "author": "lieene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lieene/typescript-textmate-grammar/issues"
  },
  "homepage": "https://github.com/lieene/typescript-textmate-grammar#readme",
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.8",
    "@types/vscode": "^1.40.0",
    "jest": "^24.9.0",
    "oniguruma": "^7.2.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "out/**/*"
  ],
  "dependencies": {
    "oniguruma-ext": "^1.1.20",
    "text-editing": "^1.0.9"
  }
}
